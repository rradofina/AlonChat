{
  "name": "alonchat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_OPTIONS=--max-old-space-size=4096 && next dev",
    "dev:windows": "taskkill /F /IM node.exe 2>nul & set NODE_OPTIONS=--max-old-space-size=4096 && next dev",
    "dev:safe": "set NEXT_PRIVATE_WORKER_THREADS=false && set NODE_OPTIONS=--max-old-space-size=4096 --unhandled-rejections=strict && next dev",
    "dev:clean": "taskkill /F /IM node.exe 2>nul & rmdir /s /q .next 2>nul & rmdir /s /q node_modules\\.cache 2>nul & set NODE_OPTIONS=--max-old-space-size=4096 && next dev",
    "clean:all": "taskkill /F /IM node.exe 2>nul & rmdir /s /q .next 2>nul & rmdir /s /q node_modules\\.cache 2>nul & del /q tsconfig.tsbuildinfo 2>nul",
    "build": "set NODE_OPTIONS=--max-old-space-size=4096 && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:generate": "supabase gen types typescript --local > lib/database.types.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset"
  },
